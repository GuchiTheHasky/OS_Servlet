<?xml version="1.0" encoding="UTF-8" ?>
<beans>

    <bean id="userService" class="the.husky.service.entity.UserService">
        <property name="userDao" ref="userDao"/>
    </bean>

    <bean id="vehicleService" class="the.husky.service.entity.VehicleService">
        <property name="vehicleDao" ref="vehicleDao"/>
    </bean>

    <bean id="securityService" class="the.husky.security.SecurityService"/>

    <bean id="cacheService" class="the.husky.service.cache.CacheService">
        <property name="userService" ref="userService"/>
        <property name="vehicleService" ref="vehicleService"/>
    </bean>

    <bean id="webService" class="the.husky.service.WebService">
        <property name="securityService" ref="securityService"/>
        <property name="cacheService" ref="cacheService"/>
    </bean>
</beans>
